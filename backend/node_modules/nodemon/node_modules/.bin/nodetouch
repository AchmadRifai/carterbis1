#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/js/carterbis1/backend/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/nodetouch.js/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/touch@3.1.0/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules:/mnt/d/js/carterbis1/backend/node_modules:/mnt/d/js/carterbis1/node_modules:/mnt/d/js/node_modules:/mnt/d/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../touch@3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
else 
  exec node  "$basedir/../../../../../touch@3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
fi
