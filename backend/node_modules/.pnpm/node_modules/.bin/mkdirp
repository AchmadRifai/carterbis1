#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/js/carterbis1/backend/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules:/mnt/d/js/carterbis1/backend/node_modules:/mnt/d/js/carterbis1/node_modules:/mnt/d/js/node_modules:/mnt/d/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules/mkdirp/bin/cmd.js/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules/mkdirp/bin/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules/mkdirp/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../mkdirp/bin/cmd.js" "$@"
fi
