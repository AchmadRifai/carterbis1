#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/js/carterbis1/backend/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/semver/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/semver@5.7.1/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules:/mnt/d/js/carterbis1/backend/node_modules:/mnt/d/js/carterbis1/node_modules:/mnt/d/js/node_modules:/mnt/d/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules/semver/bin/semver/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules/semver/bin/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules/semver/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver" "$@"
else 
  exec node  "$basedir/../semver/bin/semver" "$@"
fi
