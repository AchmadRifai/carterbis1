#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/js/carterbis1/backend/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/bin.js/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/is-ci@2.0.0/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules:/mnt/d/js/carterbis1/backend/node_modules:/mnt/d/js/carterbis1/node_modules:/mnt/d/js/node_modules:/mnt/d/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
else 
  exec node  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
fi
