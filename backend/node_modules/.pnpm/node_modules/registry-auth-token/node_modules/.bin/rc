#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/js/carterbis1/backend/node_modules/.pnpm/rc@1.2.8/node_modules/rc/cli.js/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/rc@1.2.8/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules:/mnt/d/js/carterbis1/backend/node_modules:/mnt/d/js/carterbis1/node_modules:/mnt/d/js/node_modules:/mnt/d/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
fi
