#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/js/carterbis1/backend/node_modules/.pnpm/ts-node@8.10.2_typescript@3.9.5/node_modules/ts-node/dist/bin-script.js/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/ts-node@8.10.2_typescript@3.9.5/node_modules/ts-node/dist/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/ts-node@8.10.2_typescript@3.9.5/node_modules/ts-node/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/ts-node@8.10.2_typescript@3.9.5/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules:/mnt/d/js/carterbis1/backend/node_modules:/mnt/d/js/carterbis1/node_modules:/mnt/d/js/node_modules:/mnt/d/node_modules:/mnt/d/js/carterbis1/backend/node_modules/ts-node/dist/bin-script.js/node_modules:/mnt/d/js/carterbis1/backend/node_modules/ts-node/dist/node_modules:/mnt/d/js/carterbis1/backend/node_modules/ts-node/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-script.js" "$@"
else 
  exec node  "$basedir/../ts-node/dist/bin-script.js" "$@"
fi
