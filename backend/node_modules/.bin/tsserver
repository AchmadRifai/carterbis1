#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/js/carterbis1/backend/node_modules/.pnpm/typescript@3.9.5/node_modules/typescript/bin/tsserver/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/typescript@3.9.5/node_modules/typescript/bin/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/typescript@3.9.5/node_modules/typescript/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/typescript@3.9.5/node_modules:/mnt/d/js/carterbis1/backend/node_modules/.pnpm/node_modules:/mnt/d/js/carterbis1/backend/node_modules:/mnt/d/js/carterbis1/node_modules:/mnt/d/js/node_modules:/mnt/d/node_modules:/mnt/d/js/carterbis1/backend/node_modules/typescript/bin/tsserver/node_modules:/mnt/d/js/carterbis1/backend/node_modules/typescript/bin/node_modules:/mnt/d/js/carterbis1/backend/node_modules/typescript/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else 
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
